plugins=(autojump aws brew compleat gem git gitfast hub heroku history-substring-search osx pyenv pylint python terraform zsh_reload zsh-syntax-highlighting)

source $ZSH/oh-my-zsh.sh

# Customize to your needs...
export PATH=/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/opt/X11/bin
